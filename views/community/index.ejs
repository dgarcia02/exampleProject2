<!DOCTYPE html>
<html lang="en" dir="ltr">
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <div class="container">
            <h1>Feed</h1>

            <% for (let i = 0; i < members.length; i++) { %>
                <div class="entry">
                    <a href="/community/<%=members[i]._id%>"><%=members[i].name%></a>
                    <img src="<%=members[i].posts.img%>" alt="">
                    <p><%=members[i].posts.date%></p>
                    <p><%=members[i].posts.entry%></p>
                    <p><%=members[i].posts.feeling%></p>
                </div>
            <% } %>

        </div>
    </body>
</html>
